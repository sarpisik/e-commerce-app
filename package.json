{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "Fullstack e-commerce application",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=html npm run test:unit --recursive --exit",
    "test:unit": "mocha --require @babel/register --require ./test/web/helpers.js --require ./test/web/dom.js --require ignore-styles 'test/**/*.test.js'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/coveralls/bin/coveralls",
    "dev": "webpack-dev-server --open --mode development --config webpack.config.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "webpack --mode=production --config webpack.config.js",
    "server": "nodemon src/backend/index.js",
    "demo": "npm run build && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarpisik/e-commerce-app.git"
  },
  "keywords": [
    "mongodb",
    "express",
    "react",
    "node",
    "fullstack",
    "e-commerce"
  ],
  "author": "Sarp IÅžIK <contact@sarpisik.com> (sarpisik.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sarpisik/e-commerce-app/issues"
  },
  "homepage": "https://github.com/sarpisik/e-commerce-app#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^1.3.2",
    "mongodb": "^3.1.13",
    "query-string": "^6.4.2",
    "react": "^16.6.0",
